[user]
  name = Cedric Han
  email = cedrichan@gmail.com
[apply]
  whitespace = fix
[core]
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.gitignore
[color]
  diff = always
[alias]
  st = status
  stat = status
  ci = commit
  co = checkout
  dif = diff
  cp = cherry-pick
[include]
  path = ~/.gitconfig_local
[push]
	default = simple
